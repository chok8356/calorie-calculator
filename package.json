{
  "name": "calorie-calculator",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "lint": "pnpm lint:code && pnpm lint:styles",
    "lint:code": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore",
    "lint:styles": "stylelint **/*.{vue,css,scss} --fix --ignore-path .gitignore",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build": "pnpm build:type-check && pnpm build:code",
    "build:code": "vite build",
    "build:type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "preview": "vite preview --port 4173",
    "pre-commit": "lint-staged",
    "deploy": "deploy.sh"
  },
  "dependencies": {
    "pinia": "^2.0.23",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.8.3",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.9.0",
    "dayjs": "^1.11.5",
    "eslint": "^8.25.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "postcss": "^8.4.17",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "~4.8.4",
    "uuid": "^9.0.0",
    "vite": "^3.1.6",
    "vitest": "^0.24.0",
    "vue-tsc": "^1.0.1",
    "xlsx": "^0.18.5"
  }
}
